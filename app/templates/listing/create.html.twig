{% extends 'base.html.twig' %}

{% block content %}
<h1>{{ 'title.listing_create'|trans }}</h1>
    {{ form_start(form) }}
    {{ form_widget(form) }}
<div class="form-group row float-sm-right">
    <input type="submit" value="{{ 'action.create_listing'|trans }}" class="btn btn-primary"/>
</div>
<div class="form-group row float-sm-left">
    <a href="{{ url('listing_index') }}" class="btn btn-secondary">
        {{ 'action.back_to_list'|trans }}
    </a>
</div>
{% endblock %}